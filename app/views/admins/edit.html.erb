<div class="admins-edit flex">
	<div class="leftside"></div>
	<main class="main">
		<h2 class="page-title">管理者情報編集</h2>
		<%= form_for @admin do |f| %>
			<div class="content">
				<span class="mail">メールアドレス</span>
				<span class="border"><%= f.text_field :email %></span>
			</div>
			<p class="show-btn clearfix"><%= f.submit "保存", class: "btn" %></p>
		<% end %>
		<%= link_to "戻る", admin_path(@admin), class: "btn" %>
	</main>
	<div class="rightside"></div>
</div>
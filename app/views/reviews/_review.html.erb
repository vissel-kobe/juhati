<tr>
	<% review_user = User.find(review.user_id) %>
	<td class="td-username flex">
		<%= review_user.family_name %><span class="space-between-name"></span><%= review_user.first_name %>
	</td>
	<td class="td-review"><%= review.body %></td>
	<td>
		<% if review_user.id == current_user.id %>
			<%= link_to "編集する", edit_album_review_path(review), class: "btn btn-primary" %>
		<% end %>
	</td>
</tr>
